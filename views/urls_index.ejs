<!DOCTYPE html>
<html lang="en">
<body>
    <!-- Write EJS code here to display shortened links -->

<table>
<% for (url in urls ) { %>
   <tr>
     <td><%= urls[url] %>
     <a href ="http://localhost:8080/urls/<%= url %>">Edit</a>
     <a href ="http://localhost:8080/urls/new"><% url %></a>
        <form method="POST" action="/urls/<%= url %>?_method=DELETE">

       <button type="submit">Delete</button>
    </form>
     </td>
   </tr>
<% } %>
</table>
</body>
</html>